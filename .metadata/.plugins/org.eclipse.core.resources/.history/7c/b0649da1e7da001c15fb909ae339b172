package teste;

public class Teste {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String a = "educar";
		String b = "ação";
		
		String s ="";
		
		for(int i = 0; i <= a.length()-1; i++ ) {
			char c = a.charAt(i);
			s += Character.toString(c);			
		}
		
		System.out.println(s.concat(b.substring(1,4)));
		
				
	}

}
