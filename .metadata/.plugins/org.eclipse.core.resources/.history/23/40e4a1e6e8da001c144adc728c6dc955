package teste;


class Tst {
	int ini = 0, fim =25;
	
	void print() {
		System.out.println(ini + fim);
	}
	
	{
		ini = fim%7;
		fim = ini *3;
	}
	
	Tst(int a, int b){
		ini += a;
		fim += b;
	}
	
	{
		ini /= 2;
		fim += 10;
	}
}


public class Teste {	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println( new Tst(4, -4) );	
	}

}
